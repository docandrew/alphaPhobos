extends layout

block title
	block ddox.defs
	block ddox.title

block navigation
	include ddox.inc.module-tree

block body
	block ddox.description

	block ddox.sections

	block ddox.members

	- if( cast(Module)info.node )
		section
			h2 Authors
			block ddox.authors

		section
			h2 License
			block ddox.license

	section
		h2 Comments
		- if (req is null)
			include inc.disqus

block copyright
	block ddox.copyright
